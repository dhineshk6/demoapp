import re

def extract_and_print_xml_tags(log_file_path, output_file_path):
    keyword = "Input XML"
    xml_tag_pattern = re.compile(r"<[^>]+>")

    with open(log_file_path, 'r') as log_file, open(output_file_path, 'w') as output_file:
        for line in log_file:
            if keyword in line:
                # Extract the part of the line after the keyword
                line_after_keyword = line.split(keyword, 1)[1]
                
                # Find all XML tags in the line after the keyword
                xml_tags = xml_tag_pattern.findall(line_after_keyword)
                
                # Write each XML tag to the output file
                for tag in xml_tags:
                    output_file.write(tag + '\n')

# Example usage
log_file_path = r'C:\path\to\your\logfile.log'  # Use raw string (r'') for Windows paths
output_file_path = r'C:\path\to\your\outputfile.txt'  # Use raw string (r'') for Windows paths
extract_and_print_xml_tags(log_file_path, output_file_path)
